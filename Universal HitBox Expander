Enabledisable.Text = "Enable"
Enabledisable.TextColor3 = Color3.fromRGB(255, 255, 255)
Enabledisable.TextSize = 14.000
Enabledisable.MouseButton1Down:connect(function()
	if _G.HitboxExpander.Enabled == true then 
		_G.HitboxExpander.Enabled = false
		Enabledisable.Text = "Enable"
	else
		_G.HitboxExpander.Enabled = true
		Enabledisable.Text = "Disable"
	end
end)


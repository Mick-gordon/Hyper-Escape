TeamCheckButt.Text = "Disable Team Check"
TeamCheckButt.TextColor3 = Color3.fromRGB(255, 255, 255)
TeamCheckButt.TextSize = 14.000
TeamCheckButt.MouseButton1Down:connect(function()
	if _G.HitboxExpander.TeamCheck  == true then 
		_G.HitboxExpander.TeamCheck = false
		TeamCheckButt.Text = "Enable Team Check"
	else
		_G.HitboxExpander.TeamCheck = true
		TeamCheckButt.Text = "Disable Team Check"
	end
end)


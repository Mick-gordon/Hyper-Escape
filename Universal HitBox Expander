game:GetService("StarterGui"):SetCore("SendNotification", {Title = "Hitbox Expander", Text = "By Mick Gordon"})
game:GetService("StarterGui"):SetCore("SendNotification", {Title = "https://discord.gg/FsApQ7YNTq", Text = "The Discord"})
_G.HitboxExpander ={ -- 
	Enabled = false,
	TeamCheck = true,
	Color = Color3.fromRGB(255, 0, 255),
	Body_Size = 5,
	Material = "ForceField",
	Transparency = 0,
}

local player = game.Players.LocalPlayer

while wait(1.1) do 
	if _G.HitboxExpander.Enabled == true then
		for i,v in next, game:GetService('Players'):GetPlayers() do
			if v.Name ~= game:GetService('Players').LocalPlayer.Name then
				pcall(function()
					if _G.HitboxExpander.TeamCheck == true then 
						if v.Team ~= player.Team then 
							v.Character.HumanoidRootPart.Size = Vector3.new(_G.HitboxExpander.Body_Size,_G.HitboxExpander.Body_Size,_G.HitboxExpander.Body_Size)
							v.Character.HumanoidRootPart.Transparency = _G.HitboxExpander.Transparency
							v.Character.HumanoidRootPart.Color = _G.HitboxExpander.Color
							v.Character.HumanoidRootPart.Material = _G.HitboxExpander.Material
							v.Character.HumanoidRootPart.CanCollide = false
						elseif v.Team == player.Team then
							v.Character.HumanoidRootPart.Size = Vector3.new(2, 2, 1)
							v.Character.HumanoidRootPart.Transparency = 1
							v.Character.HumanoidRootPart.Color = Color3.fromRGB(255, 255, 255)
						end
					else
						v.Character.HumanoidRootPart.Size = Vector3.new(_G.HitboxExpander.Body_Size,_G.HitboxExpander.Body_Size,_G.HitboxExpander.Body_Size)
						v.Character.HumanoidRootPart.Transparency = _G.HitboxExpander.Transparency
						v.Character.HumanoidRootPart.Color = _G.HitboxExpander.Color
						v.Character.HumanoidRootPart.Material = _G.HitboxExpander.Material
						v.Character.HumanoidRootPart.CanCollide = false
					end
				end)
			end
		end
	else
		for i,v in next, game:GetService('Players'):GetPlayers() do
			if v.Name ~= game:GetService('Players').LocalPlayer.Name then
				pcall(function()
					v.Character.HumanoidRootPart.Size = Vector3.new(2, 2, 1)
					v.Character.HumanoidRootPart.Transparency = 1
					v.Character.HumanoidRootPart.Color = Color3.fromRGB(255, 255, 255)
				end)
			end
		end
	end
end


